name: Docker Image CI

on:
  workflow_dispatch:

jobs:
 
  build:

    runs-on: ubuntu-latest

    steps:
    - uses: actions/checkout@v4
    - name: Build the Docker image
      run: docker build . -t UnknownXYZ/sample
    - name: login
      run: docker login -u UnknownXYZ -p H_-27k24gDjCTye && docker push UnknownXYZ/sample
